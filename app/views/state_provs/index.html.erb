<p id="notice"><%= notice %></p>

<h1>Listing State Provs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @state_provs.each do |state_prov| %>
      <tr>
        <td><%= state_prov.name %></td>
        <td><%= link_to 'Show', state_prov %></td>
        <td><%= link_to 'Edit', edit_state_prov_path(state_prov) %></td>
        <td><%= link_to 'Destroy', state_prov, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New State prov', new_state_prov_path %>
