<p id="notice"><%= notice %></p>

<h1>Listing Gigs</h1>

<table>
  <thead>
    <tr>
      <th>Chair</th>
      <th>Room</th>
      <th>Rank</th>
      <th>Start date</th>
      <th>End date</th>
      <th>Job number</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @gigs.each do |gig| %>
      <tr>
        <td><%= gig.chair %></td>
        <td><%= gig.room %></td>
        <td><%= gig.rank %></td>
        <td><%= gig.start_date %></td>
        <td><%= gig.end_date %></td>
        <td><%= gig.job_number %></td>
        <td><%= link_to 'Show', gig %></td>
        <td><%= link_to 'Edit', edit_gig_path(gig) %></td>
        <td><%= link_to 'Destroy', gig, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Gig', new_gig_path %>
