<% if notice %>
<p id="notice"><%= notice %></p>
<% end %>
          <div id="types_table" class="body_column one_hundred">
            <div class="column_header">
            	Listing Contract Statuses
            </div>
            <div class="body_column_content" id="availability_types_form">


<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contract_statuses.each do |contract_status| %>
      <tr>
        <td><%= contract_status.name %></td>
        <td><%= link_to 'Show', contract_status %></td>
        <td><%= link_to 'Edit', edit_contract_status_path(contract_status) %></td>
        <td><%= link_to 'Destroy', contract_status, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contract status', new_contract_status_path, class: "new_button" %>
</div></div>
