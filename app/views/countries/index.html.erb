<% if notice %>
<p id="notice"><%= notice %></p>
<% end %>
          <div id="types_table" class="body_column one_hundred">
            <div class="column_header">
            	Listing Countries
            </div>
            <div class="body_column_content" id="availability_types_form">


<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @countries.each do |country| %>
      <tr>
        <td><%= country.name %></td>
        <td><%= link_to 'Show', country %></td>
        <td><%= link_to 'Edit', edit_country_path(country) %></td>
        <td><%= link_to 'Destroy', country, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Country', new_country_path, class: "new_button" %>

</div></div>
